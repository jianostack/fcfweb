{
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        }
    ],
    "require": {
        "wpackagist-plugin/pdf-embedder":"^4.6",
        "wpackagist-plugin/polylang":"^3.0",
        "wpackagist-plugin/contact-form-7":"^5.1.9",
        "wpackagist-plugin/easy-wp-smtp": "^2.1.2",
        "league/csv": "^9.0",
        "wpackagist-plugin/ticket-tailor":"^1.10",
        "wpackagist-theme/twentytwenty":"2.1",
        "wpackagist-plugin/wp-2fa": "^2.5",
        "wpackagist-plugin/jetpack-protect": "^1.4"
    },
    "autoload": {
        "psr-0": {
            "Acme": "src/"
        }    
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "johnpbloch/wordpress-core-installer": true
        }
    }
}
